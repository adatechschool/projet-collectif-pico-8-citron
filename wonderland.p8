pico-8 cartridge // http://www.pico-8.com
version 38
__lua__
--jeu d'aventure
--par apolline

function _init()
	create_player()
	spawn_enemies()
	bullets={}
end
	
function _update()
	 player_movement()
	 update_bullets()
end 
 
function _draw()
	cls()
	draw_map()
	draw_player()
	draw_bullets()
	draw_enemies()
	end



-->8
--map

function draw_map()
	map(0,0,0,0,16,12)
	end



-->8
--player

function create_player()
	sprite=1
	p={
	x=60, --starting position
	y=60,
	speed=4
	}
	gravity=2
	j_force=20
	friction=0.85 --slow movement
end

function draw_player()
spr(1,p.x,p.y)
end

function is_grounded()
	sprite=mget(flr(p.x+4)/8,flr(p.y)/8+1)
	return fget(sprite,0)
end

function player_movement()

p.dx=0
if(btnp(➡️)) p.x+=1
if(btnp(⬅️)) p.x-=1
if(btnp(⬆️)) p.y-=j_force
if(btn(❎)) shoot()

if (is_grounded()) then
	p.dy=0
	else
	p.dy=0.98
end
p.y+=p.dy
end
-->8
--bullets

function shoot()
	new_bullet={
		x=p.x,
		y=p.y,
		speed=4
	}
	add(bullets,new_bullet)
end

function draw_bullets()
		for b in all(bullets) do
	spr(25,b.x,b.y)
	end
end

function update_bullets()
	for b in all(bullets) do
	b.x+=b.speed
	end
end
-->8
--enemies

function spawn_enemies()
	new_enemy={
	x=60,
	y=10,
	life=4
	}
	add(enemies,new_enemy)
end
	
function draw_enemies()
for e in all(enemies) do
	spr(48,e.x,e.y)
end
